--<< workbench / do / remote / synchronize / push
	
	test "${#}" -eq 0
	
	_scratch="${__REMOTE__WORKBENCH__SCRATCH}/${__REMOTE__WORKBENCH__IDENTIFIER}"
	_scratch="$( exec -- readlink -e -- "${_scratch}" )"
	
	_local_sources=.
	_remote_sources="${_scratch}/sources"
	
	_rsync_arguments=(
			
			--recursive
			--checksum --checksum-choice md5
			--times --perms
			--chmod D00500,F00400
			
			--links
			--copy-unsafe-links
			
			--itemize-changes
			
			--include '/Cargo.toml'
			--include '/Cargo.lock'
			--include '/sources'
			--include '/tests'
			--include '/examples'
			--include '/documentation'
			
			--exclude '/*'
			
			--filter 'P /.scratch'
			--filter 'P /target'
			
		)
	
	rsync \
			--delete \
			--delete-excluded \
			"${_rsync_arguments[@]}" \
			-- \
			"${_local_sources}/" \
			"${__REMOTE__SSH_TARGET}:${_remote_sources}/" \
	#
!!

