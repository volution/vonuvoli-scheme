--<< workbench / do / remote / prepare / delegate
	
	test "${#}" -eq 0
	
	test -d "${__REMOTE__WORKBENCH__SCRATCH}"
	
	_scratch="${__REMOTE__WORKBENCH__SCRATCH}/${__REMOTE__WORKBENCH__IDENTIFIER}"
	_scratch="$( exec -- readlink -f -- "${_scratch}" )"
	
	if test ! -d "${_scratch}" ; then
